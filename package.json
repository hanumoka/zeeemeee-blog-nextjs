{
  "name": "sebure-front",
  "version": "0.1.0",
  "private": true,
  "author": "hanumoka",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect --max_old_space_size=4096' next dev",
    "devmac": "NODE_OPTIONS='--inspect --max_old_space_size=4096' next dev",
    "build": "cross-env ANALYZE=true NODE_ENV=development next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
